{
    "name": "dynart/press",
    "description": "Another blog engine",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dynart\\Press\\": "src/",
            "Dynart\\Press\\Admin\\": "admin/src/",
            "Dynart\\Press\\Plugin\\": "content/plugins/"
        }
    },
    "authors": [
        {
            "name": "gopher",
            "email": "gopher.hu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "dynart/micro": "@dev",
        "dynart/micro-entities": "@dev",
        "firebase/php-jwt": "dev-main",
        "phpseclib/phpseclib": "~2.0"
    },
    "repositories": {
        "dynart-micro": {
            "type": "path",
            "url": "../dynart-micro",
            "options": {
                "symlink": true
            }
        },
        "dynart-micro-entities": {
            "type": "path",
            "url": "../dynart-micro-entities",
            "options": {
                "symlink": true
            }
        }        
    }
}
